{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/$1" }],
  "functions": {
    "src/pages/api/webhook.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 10
    }
  }
}
